{
  "expo": {
    "name": "Kandha Vilas Kitchen",
    "slug": "kandha-vilas-kitchen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-image-final.png",
    "scheme": "kandha-vilas-kitchen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "extra": {
      "eas": {
        "projectId": "5324e4f5-7944-4595-a7be-c095948e12c8"
      }
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icom-image-final.png",
        "backgroundColor": "#165016"
      },
      "package": "com.kvkcovai.foodapp",
      "googleServicesFile": "./google-services.json"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "packagingOptions": {
              "pickFirst": [
                "lib/**/libc++_shared.so",
                "lib/**/libfbjni.so"
              ]
            },
            "usesCleartextTraffic": true,
            "enableMinifyInReleaseBuilds": true
          }
        }
      ]
    ],

    "owner": "dineshmani123"
  }
}
