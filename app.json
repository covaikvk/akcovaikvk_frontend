{
  "expo": {
    "name": "Kandha Vilas Kitchen",
    "slug": "kandha-vilas-kitchen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-image-final.png",
    "scheme": "kandha-vilas-kitchen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "extra": {
      "eas": {
        "projectId": "5324e4f5-7944-4595-a7be-c095948e12c8"
      }
    },

    "android": {
      "package": "com.kandhavilas.kitchen",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-image-final.png",
        "backgroundColor": "#165016"
      },
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
