{
  "cli": {
    "version": ">= 3.13.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "ORG_GRADLE_PROJECT_android_newArchEnabled": "true",
        "ORG_GRADLE_PROJECT_expo_newArchEnabled": "true"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production": {
      "distribution": "internal",
      "env": {
        "ORG_GRADLE_PROJECT_android_newArchEnabled": "true",
        "ORG_GRADLE_PROJECT_expo_newArchEnabled": "true"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}